<?xml version='1.0'?>
<RetrospectProgramInfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.retrospect.com/RetrospectProgramInfo_v1_0.xsd" version="1.1">
    <selectorsList>
        <selector id="Block_Level_Filter">
            <name xml:lang="en">Block Level Filter</name>
            <includeConditions>
                <orConditions>
                    <sizeUsed for="files" operatorType="number" operator="greater than or equal" units="MB">100.0 MB</sizeUsed>
                </orConditions>
            </includeConditions>
            <excludeConditions>
                <orConditions>
                    <name for="files" operatorType="string" operator="ends with">.avi</name>
                </orConditions>
                <orConditions>
                    <name for="files" operatorType="string" operator="ends with">.m4v</name>
                </orConditions>
                <orConditions>
                    <name for="files" operatorType="string" operator="ends with">.mov</name>
                </orConditions>
                <orConditions>
                    <name for="files" operatorType="string" operator="ends with">.mpg</name>
                </orConditions>
                <orConditions>
                    <name for="files" operatorType="string" operator="ends with">.psd</name>
                </orConditions>
                <orConditions>
                    <name for="files" operatorType="string" operator="ends with">.pptx</name>
                </orConditions>
                <orConditions>
                    <name for="files" operatorType="string" operator="ends with">.xlsx</name>
                </orConditions>
                <orConditions>
                    <name for="files" operatorType="string" operator="ends with">.docx</name>
                </orConditions>
                <orConditions>
                    <name for="files" operatorType="string" operator="ends with">.zip</name>
                </orConditions>
            </excludeConditions>
        </selector>
    </selectorsList>
</RetrospectProgramInfo>
